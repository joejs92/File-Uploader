<nav>
  <h1><%= title %></h1>
  <% if (locals.user){%> 
    <ul>
      <li><p>Hello, <%= user.username%></p></li>
    </ul>
  <%} else {%> 
      <ul>
      <% for (let i = 0; i < links.length; i++) { %>
        <li>
          <a href="<%= links[i].href %>">
            <span> <%= links[i].text %> </span>
          </a>
        </li>
      <% } %>
    </ul>
  <%} %>
</nav>